@model IEnumerable<PdfCertificado.Models.User>
@using PdfCertificado.Models.Services;

@{
	ViewData["Title"] = "Pdfview";
}

<h1 class="mt-3">Bem vindo Administrador</h1>


<div class="field-error">
	<h3>Bem vindo <strong>@TempData["UsuarioLogado"]</strong></h3>
</div>
<p>
	<a asp-action="Logout">Logout session</a>
</p>


<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Username)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Email)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Password)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Username)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Email)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Password)
				</td>
				<td>
					<a asp-action="Edit" asp-route-id="@item.Id">Edit</a> 
					<a asp-action="Details" asp-route-id="@item.Id">Details</a> 
					<a asp-action="DeleteUser" asp-route-id="@item.Id">Delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>
